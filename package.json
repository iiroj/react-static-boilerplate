{
  "name": "react-static-boilerplate",
  "version": "2.3.2",
  "description": "An example React static site with SSR and code-splitting",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": "gitlab:iiroj/react-static-boilerplate",
  "bugs": "https://gitlab.com/iiroj/react-static-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=node webpack-dev-server",
    "lint": "eslint .",
    "prebuild": "rimraf public",
    "build": "NODE_ENV=production BABEL_ENV=node webpack --config webpack.config.babel.js",
    "postbuild": "cp -r static/. public/",
    "version": "standard-version",
    "prerelease": "npm run lint && npm run build",
    "release": "git push --follow-tags origin master",
    "start": "serve public"
  },
