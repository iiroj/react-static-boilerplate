    "react-universal-component": "^3.0.3",
    "serve": "^10.0.2",
    "styled-components": "^4.0.0-beta.9"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
