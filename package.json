{
  "name": "react-static-boilerplate",
  "version": "5.4.13",
  "description": "An example React static site with SSR and code-splitting",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": "github:iiroj/react-static-boilerplate",
  "bugs": "https://github.com/iiroj/react-static-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=node_development webpack-dev-server",
    "lint": "eslint .",
    "prebuild": "rimraf public",
    "build": "NODE_ENV=production BABEL_ENV=node_production webpack",
    "stats": "webpack-bundle-analyzer public/stats.json",
    "start": "serve public",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@loadable/component": "^5.12.0",
    "core-js": "^3.6.4",
    "css-wipe": "^4.3.0",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.10.1",
    "react": "^16.13.0",
    "react-dom": "^16.13.0",
    "react-helmet-async": "^1.0.4",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "react-style-reset": "^1.3.1",
    "serve": "^11.3.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.7",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.8.7",
    "@babel/preset-react": "^7.8.3",
    "@babel/register": "^7.8.6",
    "@commitlint/config-conventional": "^8.3.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@loadable/babel-plugin": "^5.12.0",
    "@loadable/server": "^5.12.0",
    "@loadable/webpack-plugin": "^5.12.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-export-default-name": "^2.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "commitlint": "^8.3.5",
    "common-tags": "^1.8.0",
    "copy-webpack-plugin": "^5.1.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react-app": "^6.2.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-renderer-webpack-plugin": "^5.1.3",
    "husky": "^4.2.3",
    "lint-staged": "^10.0.8",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.2",
    "standard-version": "^7.1.0",
    "stats-webpack-plugin": "^0.7.0",
    "terser-webpack-plugin": "^2.3.5",
    "typescript": "^3.8.3",
    "webpack": "^4.42.0",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3"
  }
}
