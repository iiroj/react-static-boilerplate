{
  "name": "react-static-boilerplate",
  "version": "5.4.10",
  "description": "An example React static site with SSR and code-splitting",
  "author": "Iiro JÃ¤ppinen <iiro@jappinen.fi> (https://iiro.fi)",
  "license": "MIT",
  "repository": "github:iiroj/react-static-boilerplate",
  "bugs": "https://github.com/iiroj/react-static-boilerplate/issues",
  "private": true,
  "scripts": {
    "dev": "NODE_ENV=development BABEL_ENV=node_development webpack-dev-server",
    "lint": "eslint .",
    "prebuild": "rimraf public",
    "build": "NODE_ENV=production BABEL_ENV=node_production webpack",
    "stats": "webpack-bundle-analyzer public/stats.json",
    "start": "serve public",
    "version": "standard-version --no-verify"
  },
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "standard-version": {
    "scripts": {
      "prerelease": "npm run lint && npm run build"
    }
  },
  "dependencies": {
    "@emotion/core": "^10.0.16",
    "@loadable/component": "^5.10.2",
    "core-js": "^3.2.1",
    "css-wipe": "^4.3.0",
    "fontfaceobserver": "^2.1.0",
    "history": "^4.9.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet-async": "^1.0.2",
    "react-router": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "react-style-reset": "^1.2.11",
    "serve": "^11.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-export-namespace-from": "^7.5.2",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.5.5",
    "@commitlint/config-conventional": "^8.1.0",
    "@emotion/babel-preset-css-prop": "^10.0.14",
    "@loadable/babel-plugin": "^5.10.0",
    "@loadable/server": "^5.10.2",
    "@loadable/webpack-plugin": "^5.7.1",
    "babel-eslint": "^10.0.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-transform-export-default-name": "^2.0.4",
    "case-sensitive-paths-webpack-plugin": "^2.2.0",
    "commitlint": "^8.1.0",
    "common-tags": "^1.8.0",
    "copy-webpack-plugin": "^5.0.4",
    "eslint": "^6.2.2",
    "eslint-config-prettier": "^6.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react-app": "^6.0.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-renderer-webpack-plugin": "^5.0.1",
    "husky": "^3.0.4",
    "lint-staged": "^9.2.5",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "standard-version": "^7.0.0",
    "stats-webpack-plugin": "^0.7.0",
    "terser-webpack-plugin": "^1.4.1",
    "typescript": "^3.5.3",
    "webpack": "^4.39.3",
    "webpack-bundle-analyzer": "^3.4.1",
    "webpack-cli": "^3.3.7",
    "webpack-dev-server": "^3.8.0"
  }
}
